{
    "manifest_version": 2,
    "name": "previews for javlib",
    "version": "0.7.0",
    "description": "配置化+分离",
    "permissions": [
        "http://*/"
    ],
    "background": {
        "scripts": [
            "config.js",
            "axios.min.js",
            "background.js"
        ]
    },
    "content_scripts": [{
            "matches": [
                "*://www.javlibrary.com/cn/?v=*",
                "*://www.javbus.com/*-*"
            ],
            "js": [
                "axios.min.js",
                "moment-with-locales.min.js",
                "config.js",
                "common.js",
                "vue.js"
            ],
            "css": ["mystyle.css"]
        },
        {
            "matches": [
                "*://www.javlibrary.com/cn/?v=*"
            ],
            "js": [
                "javlib.js"
            ]
        },
        {
            "matches": [
                "*://www.javbus.com/*-*"
            ],
            "js": [
                "javbus.js"
            ]
        }
    ]
}